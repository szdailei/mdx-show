{
  "name": "dailei-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format": "prettier --ignore-path .gitignore --config prettier.config.cjs --write .",
    "lint": "eslint --config .eslintrc.cjs . --ext .js,.jsx,.mjs,.cjs,.ts,.tsx",
    "stylelint": "stylelint --config stylelint.config.cjs --config-basedir . \"src/**/*.css\"",
    "check": "tsc",
    "analyze": "echo run 'source-map-explorer dist/client/index.[hash].js dist/client/index.[hash].js.map --no-border-checks --only-mapped'",
    "css": "tailwindcss -i ./src/client/static/index.css -o ./dist/output.css ",
    "load-test": "wrk -t12 -c400 -d30s --latency http://localhost:3000/index.html",
    "rel": "tauri build",
    "rel-dev": "tauri build --debug"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.1.0",
    "acorn": "^8.8.1",
    "acorn-jsx": "^5.3.2",
    "astring": "^1.8.4",
    "estree-util-build-jsx": "^2.2.0",
    "html-tags": "^3.2.0",
    "htmlparser2": "^8.0.1",
    "mdast-util-from-markdown": "^1.2.0",
    "mdast-util-mdxjs-esm": "^1.3.0",
    "micromark": "^3.1.0",
    "micromark-extension-gfm": "^2.0.1",
    "micromark-extension-mdxjs-esm": "^1.0.3",
    "mime": "^3.0.0",
    "minimist": "^1.2.7",
    "quick-lru": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.4.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.8",
    "@tauri-apps/cli": "^1.1.0",
    "@types/domhandler": "^2.4.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "arkit": "^1.6.4",
    "esbuild": "^0.16.4",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.20",
    "prettier": "^2.6.2",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^14.15.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^29.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  }
}
