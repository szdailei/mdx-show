{
  "author": {
    "name": "Dai Lei",
    "email": "szdailei@gmail.com"
  },
  "description": "Write with MDX, slides show with browser",
  "exports": "./dist/index.js",
  "keywords": [
    "mdx",
    "presentation",
    "show",
    "slides"
  ],
  "license": "MIT",
  "name": "mdx-show",
  "private": true,
  "repository": {
    "url": "https://github.com/szdailei/mdx-show"
  },
  "homepage": "https://github.com/szdailei/mdx-show#readme",
  "main": "dist/index.js",
  "type": "module",
  "version": "0.1.0",
  "workspaces": [
    "packages/*",
    "test"
  ],
  "files": [
    "dist",
    "example"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write packages/**/* src/**/* scripts/**/* test/**/*",
    "lint": "eslint packages src scripts test",
    "stylelint": "stylelint --syntax \"css-in-js\" \"packages/client/src/**/*.{jsx,tsx}\"",
    "clean": "rimraf dist/",
    "build": "node --experimental-specifier-resolution=node scripts/rollup/build.js",
    "start": "node dist/index.js",
    "debug": "DEBUG=* node dist/debug.js",
    "bundle": "node --experimental-specifier-resolution=node scripts/bundle/bundle.js"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@stitches/react": "^1.2.5",
    "loglevel": "^1.7.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "babel-plugin-transform-import-meta": "^2.1.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "pkg": "^5.3.2",
    "pkg-fetch": "^3.2.3",
    "postcss": "^8.3.11",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.55.1",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-postcss": "^4.0.1",
    "shelljs": "^0.8.4",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^21.0.0",
    "web-resource-inliner": "^5.0.0"
  }
}
