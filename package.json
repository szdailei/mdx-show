{
  "name": "mdx-show",
  "version": "0.7.0",
  "description": "Write with MDX, slides show with browser",
  "author": "Dai Lei <szdailei@gmail.com>",
  "keywords": [
    "mdx",
    "presentation",
    "show",
    "slides"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/szdailei/mdx-show.git"
  },
  "homepage": "https://github.com/szdailei/mdx-show#readme",
  "main": "server.js",
  "type": "module",
  "bin": {
    "mdx-show": "cli.mjs",
    "mdx-show-pdfs": "cli-pdfs.mjs"
  },
  "files": [
    "dist/web/index.html",
    "dist/web/app.js",
    "dist/server.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.mjs,.cjs,.ts,.tsx",
    "stylelint": "stylelint \"src/client/**/*.{jsx,tsx}\"",
    "build": "cross-env NODE_ENV=production node node_modules/@szdailei/dev-scripts/cli/cli-build.js",
    "build:dev": "cross-env NODE_ENV=development node node_modules/@szdailei/dev-scripts/cli/cli-build.js",
    "watch": "cross-env NODE_ENV=development node node_modules/@szdailei/dev-scripts/cli/cli-watch.js",
    "release-info": "node node_modules/@szdailei/dev-scripts/cli/cli-gen-release-note.js",
    "to-github": "node node_modules/@szdailei/dev-scripts/cli/cli-to-github.js",
    "to-npm": "node node_modules/@szdailei/dev-scripts/cli/cli-to-npm.js",
    "exe": "node node_modules/@szdailei/dev-scripts/cli/cli-exe.js",
    "server": "node cli.mjs",
    "pdfs": "node cli-pdfs.mjs",
    "work": "node work-flow/cli-work-flow.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest && chromium reports/jest_html_reporters.html"
  },
  "dependencies": {
    "@stitches/react": "^1.2.5",
    "@szdailei/core": "^0.1.0",
    "history": "^5.1.0",
    "html-parse-stringify": "^3.0.1",
    "marked": "^3.0.8",
    "mime": "^3.0.0",
    "minimist": "^1.2.5",
    "prismjs": "^1.25.0",
    "prop-types": "^15.7.2",
    "quick-lru": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.0-beta.5"
  },
  "devDependencies": {
    "@szdailei/dev-scripts": "^0.1.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.4.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
