{
  "name": "mdx-show",
  "version": "0.3.0",
  "description": "Write with MDX, slides show with browser",
  "author": "Dai Lei <szdailei@gmail.com>",
  "keywords": [
    "mdx",
    "presentation",
    "show",
    "slides"
  ],
  "license": "MIT",
  "repository": {
    "url": "https://github.com/szdailei/mdx-show"
  },
  "homepage": "https://github.com/szdailei/mdx-show#readme",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mdx-show": "cli.js"
  },
  "files": [
    "dist/web",
    "dist/index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write packages/**/* src/**/* scripts/**/* test/**/*",
    "lint": "eslint packages src scripts test --ext .js,.jsx,.mjs",
    "stylelint": "stylelint --syntax \"css-in-js\" \"packages/client/src/**/*.{jsx,tsx}\"",
    "clean": "rimraf dist/ pdfs/ reports/",
    "build": "cross-env NODE_ENV=production node scripts/rollup/build.js",
    "dev": "cross-env NODE_ENV=development node scripts/rollup/build.js",
    "start": "node dist/index.js",
    "debug": "DEBUG=* node dist/debug.js",
    "bundle": "node scripts/bundle/bundle.js",
    "release": "node scripts/release/release.js",
    "export": "node --experimental-specifier-resolution=node test/export-slides.js",
    "test": "node --experimental-vm-modules --experimental-specifier-resolution=node node_modules/.bin/jest && chromium reports/jest_html_reporters.html"
  },
  "dependencies": {
    "@stitches/react": "^1.2.5",
    "graphql": "^15.6.1",
    "history": "^5.0.1",
    "is-alphabetical": "^2.0.0",
    "loglevel": "^1.7.1",
    "marked": "^3.0.8",
    "mime": "^2.5.2",
    "prismjs": "^1.25.0",
    "prop-types": "^15.7.2",
    "quick-lru": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.0-beta.5",
    "video.js": "^7.15.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@iarna/toml": "^2.2.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "babel-plugin-transform-import-meta": "^2.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.3.1",
    "jest-html-reporters": "^2.1.6",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "node-fetch": "^3.0.0",
    "pdf-lib": "^1.17.0",
    "pkg": "^5.3.2",
    "pkg-dir": "^6.0.1",
    "pkg-fetch": "^3.2.3",
    "postcss": "^8.3.11",
    "prettier": "^2.3.2",
    "puppeteer-core": "^10.4.0",
    "read-pkg-up": "^9.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.55.1",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-postcss": "^4.0.1",
    "shelljs": "^0.8.4",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^21.0.0",
    "web-resource-inliner": "^5.0.0"
  }
}
