{
  "name": "mdx-show",
  "version": "0.9.2",
  "type": "module",
  "description": "Write with MDX, slides show with browser",
  "author": "Dai Lei <szdailei@gmail.com>",
  "keywords": [
    "mdx",
    "slides"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/szdailei/mdx-show.git"
  },
  "homepage": "https://github.com/szdailei/mdx-show#readme",
  "main": "dist/cli.js",
  "bin": {
    "mdx-show": "dist/server/cli.js",
    "mdx-show-pdf": "dist/pdf/cli.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "format": "prettier --ignore-path .gitignore --config prettier.config.cjs --write .",
    "lint": "eslint --config .eslintrc.cjs . --ext .js,.jsx,.mjs,.cjs,.ts,.tsx",
    "stylelint": "stylelint --config stylelint.config.cjs --config-basedir . \"src/**/*.css\"",
    "check": "tsc",
    "analyze": "echo run 'source-map-explorer dist/client/index.[hash].js dist/client/index.[hash].js.map --no-border-checks --only-mapped'",
    "css": "tailwindcss -i ./src/client/static/index.css -o ./dist/output.css ",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "load-test": "wrk -t12 -c400 -d30s --latency http://localhost:3000/index.html",
    "rel": "tauri build",
    "rel-dev": "tauri build --debug",
    "pdf": "src-pdf/cli.js",
    "to-main": "scripts/cli/cli-dev-to-main.js",
    "release": "scripts/cli/cli-gen-release-note.js",
    "to-github": "scripts/cli/cli-to-github.js",
    "to-npm": "scripts/cli/cli-to-npm.js"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.1.0",
    "acorn": "^8.8.1",
    "acorn-jsx": "^5.3.2",
    "astring": "^1.8.4",
    "estree-util-build-jsx": "^2.2.0",
    "html-tags": "^3.2.0",
    "htmlparser2": "^8.0.1",
    "mdast-util-from-markdown": "^1.2.0",
    "mdast-util-mdxjs-esm": "^1.3.0",
    "micromark": "^3.1.0",
    "micromark-extension-gfm": "^2.0.1",
    "micromark-extension-mdxjs-esm": "^1.0.3",
    "mime": "^3.0.0",
    "minimist": "^1.2.7",
    "quick-lru": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.4.3"
  },
  "devDependencies": {
    "@octokit/rest": "^19.0.7",
    "@tailwindcss/typography": "^0.5.8",
    "@tauri-apps/cli": "^1.1.0",
    "@types/domhandler": "^2.4.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "arkit": "^1.6.4",
    "conventional-changelog-core": "^4.2.4",
    "esbuild": "^0.16.17",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.2",
    "jest-html-reporters": "^3.1.3",
    "pdf-lib": "^1.17.1",
    "postcss": "^8.4.20",
    "prettier": "^2.6.2",
    "puppeteer-core": "^19.7.0",
    "shelljs": "^0.8.5",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^14.15.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^29.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  }
}
