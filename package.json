{
  "name": "mdx-show",
  "version": "0.7.0",
  "description": "Write with MDX, slides show with browser",
  "author": "Dai Lei <szdailei@gmail.com>",
  "keywords": [
    "mdx",
    "presentation",
    "show",
    "slides"
  ],
  "license": "MIT",
  "repository": "github:szdailei/mdx-show",
  "homepage": "https://github.com/szdailei/mdx-show#readme",
  "main": "server.js",
  "type": "module",
  "bin": {
    "mdx-show": "cli.mjs",
    "mdx-show-pdfs": "cli-pdfs.mjs"
  },
  "files": [
    "dist/web/",
    "dist/server.js",
    "server.js",
    "pdfs.js",
    "src/",
    "test/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write src/**/* scripts/**/* test/**/*",
    "lint": "eslint src scripts test --ext .js,.jsx,.mjs",
    "stylelint": "stylelint \"src/client/**/*.{jsx,tsx}\"",
    "clean": "rimraf dist/ pdfs/ reports/",
    "build": "cross-env NODE_ENV=production node scripts/rollup/cli-build.js",
    "build:dev": "cross-env NODE_ENV=development node scripts/rollup/cli-build.js",
    "watch": "cross-env NODE_ENV=development node scripts/watch/cli-watch.js",
    "server": "node cli.mjs",
    "pdfs": "node cli-pdfs.mjs",
    "work": "node work-flow/cli-work-flow.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest && chromium reports/jest_html_reporters.html",
    "exe": "node scripts/exe/cli-exe.js",
    "release-info": "node scripts/publish/gen-release-note.js",
    "to-github": "node scripts/publish/to-github.js",
    "to-npm": "node scripts/publish/to-npm.js"
  },
  "dependencies": {
    "@stitches/react": "^1.2.5",
    "history": "^5.1.0",
    "html-parse-stringify": "^3.0.1",
    "marked": "^3.0.8",
    "mime": "^3.0.0",
    "minimist": "^1.2.5",
    "prismjs": "^1.25.0",
    "prop-types": "^15.7.2",
    "quick-lru": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.0-beta.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@iarna/toml": "^2.2.5",
    "@octokit/rest": "^18.12.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "babel-plugin-transform-import-meta": "^2.1.0",
    "chokidar": "^3.5.2",
    "commitizen": "^4.2.4",
    "conventional-changelog": "^3.1.24",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.3.1",
    "jest-html-reporters": "^2.1.6",
    "mkdirp": "^1.0.4",
    "pdf-lib": "^1.17.0",
    "pkg": "^5.3.2",
    "pkg-dir": "^6.0.1",
    "pkg-fetch": "^3.2.3",
    "postcss": "^8.3.11",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.3.2",
    "puppeteer-core": "^10.4.0",
    "react-flow-renderer": "^9.6.9",
    "read-pkg-up": "^9.0.0",
    "rfdc": "^1.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.55.1",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "shelljs": "^0.8.4",
    "stylelint": "^14.0.1",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-standard": "^23.0.0",
    "web-resource-inliner": "^5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
